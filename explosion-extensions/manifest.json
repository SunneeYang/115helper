{
    "manifest_version": 3,
    "name": "特定网站链接打开器",
    "version": "1.0",
    "permissions": ["activeTab", "tabs", "scripting"],
    "host_permissions": [
        "*://*.xxx.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  